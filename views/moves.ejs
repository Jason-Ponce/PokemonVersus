<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../views/partials/head') %>
    <script defer src="/js/home.js"></script>
    <title>PokeDex</title>
</head>
<body>
    <%- include('../views/partials/header') %>
    <%- include('../views/partials/pokebelt') %>
    <%- include('../views/partials/bag') %>
    <main class="main">
        <div class="wrapper">
            <h1>Moves</h1>
            <div class="moves">
                <table>
                    <tr>
                        <td class="type">Type</td>
                        <td class="type-data">
                            <%= moves.type %>
                        </td>
                    </tr>
                    <tr>
                        <td class="category">Category</td>
                        <td class="category-data">
                            <%= moves.category %>
                        </td>
                    </tr>
                    <tr>
                        <td class="pp">PP</td>
                        <td class="pp-data">
                            <%= moves.pp %>
                        </td>
                    </tr>
                    <tr>
                        <td class="power">Power</td>
                        <td class="power-data">
                            <%= moves.power %>
                        </td>
                    </tr>
                    <tr>
                        <td class="accuracy">Accuracy</td>
                        <td class="accuracy-data">
                            <%= moves.accuracy %>
                        </td>
                    </tr>
                </table>
                <div class="flavor_text-data">
                    <%= moves.flavor_text %>
                </div>
            </div>
        </div>
    </main>
</body>
</html>